<!DOCTYPE html>
<html>
  <head>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400&display=swap" rel="stylesheet">
    <style>
        *{
            margin: 0;
        }
        .avv{
          display: flex;
        }
        body{
            padding: 0px 40px;
            font-size: 20px;
            padding-top: -50px !important;
            font-family: 'Roboto', sans-serif;
            color:rgb(93, 95, 95);
           
            
        }
      table {
        width: 100%;
        table-layout:fixed;
        padding-bottom: 25px;
      }
      td > span {
          font-size:1.8rem
          
      }
      td {
        word-wrap:break-word;
        padding: 10px 2px;
        
      }
      td > b {
        color: rgb(46, 48, 48) !important;
      }
      td > h3 {
        color: white !important;
        padding-left: 35px;
        padding-top: 7px;
        padding-bottom: 7px;
        border-radius: 20px !important;
        border-collapse: separate;
      }
     hr{
         border: 1px solid  rgb(138, 134, 134);
     }
      tr{
          padding: 14px;
      }
      p {
        font-family: sans-serif;
        font-size: 30px !important;
        padding-bottom: 8px;
        font-weight: 800;
        border-bottom: 2px solid rgb(138, 134, 134);
        color: rgb(108, 112, 112) !important;
      }
      td > p{
          color: rgb(14, 168, 84) !important;
          padding-bottom: 0px !important;
      }
      td > strong {
          font-size: 30px;
          font-family: sans-serif;
          color:rgb(108, 112, 112) !important;
      }

      table > tr > td > img {
        height: 200px;
        width: auto;
      }
      #cssTable td 
{
    text-align: center; 
    vertical-align: middle;
}
    
    </style>
  </head>
  <body>
    <div class="header">
        <table>
          <tr>
            <td width="60%">
              <img
                src="data:image/png;base64,"
                alt=""
              />
            </td>
            <td align="right" width="40%">Report from <%=reports.report_period%></td>
          </tr>
        </table>
        <table style="padding-bottom: 0px !important;">
            <tr>
                <td width="55%" style="font-weight: 800;">
                  <span>Payments and Salary</span> 
                    
                </td>
                <td width="45%" style="font-weight: 800;"
                   style="margin-left:50px;"><span>Total Due:</span><span style="color: green;"> Rp <%=due.toLocaleString(
                    undefined,
                    { minimumFractionDigits: 0 }
                  ).replace(/,/g, '.')%></span>
                </td>
            </tr> 
        </table>
        <hr>
        <table style="margin-top: 3%;">
            <tr bgcolor="#ccffff">
                <td width="20%"><b>EMPLOYEE NAME</b></td>
                <td width="20%"><b>SALARY EARNED</b></td>
                <td width="25%"><b>PREVIOUS BALANCE</b></td>
                <td width="20%"><b>PAYMENTS MADE</b></td>
                <td width="15%"><b>TOTAL DUE</b></td>
            </tr>
            <% reports.reports.forEach((report,index) => {%>
            
                <tr class="header__monthly">
                  <td><%=report.information.employee_name%></td>
                  <td style="color: green;">Rp <%=report.salary.net_salary.toLocaleString(
                    undefined,
                    { minimumFractionDigits: 0 }
                  ).replace(/,/g, '.')%></td>
                  <td style="color: green;">Rp <%=report.salary.previous_balance.toLocaleString(
                    undefined,
                    { minimumFractionDigits: 0 }
                  ).replace(/,/g, '.')%></td>
                  <td style="color: red;">Rp <%=report.salary.payments.toLocaleString(
                    undefined,
                    { minimumFractionDigits: 0 }
                  ).replace(/,/g, '.')%></td>
                  <td style="color: green;">Rp <%=report.salary.total_payable.toLocaleString(
                    undefined,
                    { minimumFractionDigits: 0 }
                  ).replace(/,/g, '.')%></td>
                </tr>
                
              <% });%>
        </table>
      </div>
  </body>
</html>
